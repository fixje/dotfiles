# starting Virtual Machines
alias openbsd='VBoxManage startvm freebsd --type headless'
alias debvm='VBoxManage startvm debian-db01 --type headless'
alias win8='VBoxManage startvm win8 --type gui'
alias gridvm='VBoxManage startvm GridCloud --type gui'

